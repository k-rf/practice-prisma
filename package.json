{
  "name": "template-typescript",
  "version": "1.0.1",
  "description": "The template of TypeScript.",
  "main": "index.ts",
  "author": "Kawano Kenichi <kenichi.k.820@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --fix --cache --ext .tsx,.ts,.js ./src",
    "format": "prettier . --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate": "dotenv -e .env.local -- prisma migrate dev"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.3.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "dotenv-cli": "^5.0.0",
    "esbuild-register": "^3.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/platform-express": "^8.3.1",
    "@prisma/client": "^3.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4"
  }
}
